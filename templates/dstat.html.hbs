<script>
    function setClipboard() {
        const tempInput = document.createElement("input");
        const value = document.getElementById("copyToClipboard").getAttribute("data-href");
        tempInput.style = "position: absolute; left: -1000px; top: -1000px";
        tempInput.value = value;
        document.body.appendChild(tempInput);
        tempInput.select();
        document.execCommand("copy");
        document.body.removeChild(tempInput);
    }

    $(document).ready(function () {
        let chart; // global

        $(window).load(function () {
            initiateChart("L7dstat");
            parseFile();
        });

        function parseFile() {
            const id = "{{ id }}";

            $.ajax({
                url: "http://127.0.0.1:8000/api/v1/data?name=" + id,
                dataType: "text",
                method: "get",
                timeout: 1000,
                error: function () {
                    console.log("Catching timeout");
                    setTimeout(parseFile, 3000);
                },
                cache: false
            }).done(
                    function (data) {
                        const series = chart.series[0];
                        const current = parseInt(data);

